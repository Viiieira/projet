<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="body" stylesheets="@styles/main.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projet.SupplierController">
   <children>
      <Accordion layoutX="127.0" layoutY="156.0" />
      <TableView fx:id="supplierTable" layoutX="275.0" layoutY="100.0" prefHeight="200.0" prefWidth="298.0">
        <columns>
          <TableColumn fx:id="idCollun" prefWidth="75.0" text="id" />
          <TableColumn fx:id="nameCollum" prefWidth="75.0" text="name" />
            <TableColumn fx:id="emailCollun" prefWidth="75.0" text="email" />
            <TableColumn fx:id="telefoneCollun" prefWidth="75.0" text="phone" />
        </columns>
      </TableView>
      <Button fx:id="adicionarFornecedorButton" layoutX="352.0" layoutY="341.0" mnemonicParsing="false" styleClass="btn" text="Adicionar Fornecedor" />
      <Button fx:id="atualizarbutton" layoutX="461.0" layoutY="35.0" mnemonicParsing="false" styleClass="btn" text="Atualizar" />
      <AnchorPane prefHeight="400.0" prefWidth="200.0" styleClass="sidebar">
         <children>
            <Button fx:id="sairButton" layoutX="10.0" layoutY="341.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="180.0" styleClass="btn" text="Sair" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
